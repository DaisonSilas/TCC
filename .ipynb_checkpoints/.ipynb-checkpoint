   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "def predicao(anls):\n",
    "    if anls >= 0.1:\n",
    "        return 1.0\n",
    "    elif (anls <= 0.1) and (anls >= -0.1):\n",
    "        return 0.0\n",
    "    else:\n",
    "        return -1.0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "def acuracia(anls, pred):\n",
    "    if (anls - pred) == 0:\n",
    "        return 1.0\n",
    "    else:\n",
    "        return 0.0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "predicao = [predicao(dados['Analise'][ind]) for ind in dados.index]"
   ]
