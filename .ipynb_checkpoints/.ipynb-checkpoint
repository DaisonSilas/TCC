   "source": [
    "def polaridade(pol):\n",
    "    if pol == 'Positivo':\n",
    "        return 1.0\n",
    "    elif pol == 'Neutro':\n",
    "        return 0.0\n",
    "    else:\n",
    "        return -1.0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "polaridades = [polaridade(dados[\"Polaridade\"][ind]) for ind in dados.index]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
