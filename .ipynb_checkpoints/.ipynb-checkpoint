   "metadata": {},
   "outputs": [],
   "source": [
    "def polaridade_textblob(text):\n",
    "    frase = TextBlob (text)\n",
    "    traduz = TextBlob(str(frase.translate(to=\"en\" )))\n",
    "    return traduz.polarity"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "polaridades = [polaridade_textblob(dados['Frase'][ind]) for ind in dados.index]"
